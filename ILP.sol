contract ImpermanentLossProtection is Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    // Info of each user.
    struct UserInfo {
        uint256 amount; // How many LP tokens the user has provided.
        uint256 rewardDebt; // Reward debt. See explanation below.
		uint256 depositTime; // Time when the user deposit LP tokens.
		uint256 depVal; // LP token value at the deposit time.
		
		 //   pending reward = (user.amount * pool.acckinsPerShare) - user.rewardDebt
    }
    
    // Info of each pool.
    struct PoolInfo {
        address lpToken;    // Address of LP token contract.
		IERC20 token0;      // Token0 accoring to the LP pair.
		IERC20 token1;      // Token1 accoring to the LP pair.
		uint256 token0_decimal; //Token0 decimals.
		uint256 token1_decimal; //Token1 decimals.
		bool impermanentLossProtection; // ILP availability
		
    }

    address public kins;
    address public devAddr;
    address public kinsMaster;
	
	PoolInfo[] public poolInfo;
	
	modifier onlyFarm() {
        require(kinsMaster == _msgSender() || owner() == _msgSender(), "ERROR: caller is not the kinsMaster or owner.");
        _;
    }
    modifier onlyDev() {
        require(kinsMaster == _msgSender() || devAddr == _msgSender(), "ERROR: caller is not the kinsMaster or Dev.");
        _;
    }

    constructor( address _kins, address _kinsMaster)
	public 
	{
        kins = _kins;
		kinsMaster = _kinsMaster;
        devAddr = msg.sender;
        
        // staking pool
        poolInfo.push(PoolInfo({
            lpToken: _kins,
            token0: IERC20(0),
            token1: IERC20(0),
            token0_decimal: 18,
		    token1_decimal: 18,
            impermanentLossProtection: false
        }));
    }
	
    function setAddresses( address _kins, address _kinsMaster ) 
	public 
	onlyFarm 
	{
        kins = _kins;
		kinsMaster = _kinsMaster;
		
		set(0,_kins,IERC20(_kins),IERC20(_kins),false);
    }
    
    
    // Safe Kins transfer function, just in case if rounding error causes pool to not have enough KINS.
    function kinsTransfer(address _to, uint256 _amount) 
	external
	onlyFarm	
	{
		uint256 kinsBal = IERC20(kins).balanceOf(address(this));
		uint256 xfAmt = _amount;
		if( xfAmt > kinsBal )
			xfAmt = kinsBal;
		if(xfAmt > 0)
			IERC20(kins).transfer(_to, xfAmt);
    }

    // Update dev address by the previous dev.
    function dev(address _devAddr) public {
        require(msg.sender == devAddr, "dev: wut?");
        devAddr = _devAddr;
    }

    function add(
		address _lpToken,
		IERC20 _token0,
		IERC20 _token1,
		bool _offerILP
    )
	public 
	onlyDev 
	{
        poolInfo.push(
            PoolInfo({
                lpToken: _lpToken,
				token0: _token0,
				token1: _token1,
				token0_decimal: _token0.decimals(),
				token1_decimal: _token1.decimals(),
				impermanentLossProtection: _offerILP
            })
        );
    }
    function set(
        uint256 _pid,
		address _lpToken,
		IERC20 _token0,
		IERC20 _token1,
		bool _offerILP
    )
	public 
	onlyDev 
	{
		poolInfo[_pid].lpToken = _lpToken;
		poolInfo[_pid].token0 = _token0;
		poolInfo[_pid].token1 = _token1;
		poolInfo[_pid].token0_decimal = _token0.decimals();
		poolInfo[_pid].token1_decimal = _token1.decimals();
		poolInfo[_pid].impermanentLossProtection = _offerILP;
    }
		
	//IMPERMANENT LOSS PROTECTION
	//Liquidty Value, KINS Price, Etc
    function getDepositValue(uint256 amount, uint256 _pid)
	external
	view
	returns (uint256 userDepValue)
    {		
		PoolInfo storage pool = poolInfo[_pid];
		return _getDepositValue(amount, pool);
    }
	
    function _getDepositValue(uint256 _amount, PoolInfo storage _pool)
	internal
	view
	returns (uint256 userDepValue)
    {		
		IUniswapPair pair = IUniswapPair(address(_pool.lpToken));
		
		if (kins == address(_pool.token0) || kins == address(_pool.token1))
		{
	    	userDepValue = (2 * (IERC20(kins).balanceOf(_pool.lpToken))
	    	                .mul(_getKinsPrice(_pool.lpToken, _pool.token0, _pool.token1,
	    	                _pool.token0_decimal, _pool.token1_decimal))                            //LPToken Price Estimate
					    	).mul(_amount).div(pair.totalSupply());                                 // UserLP / TotalLP
		}
		else 
		{
		    userDepValue = 0;
		}
		
		return userDepValue;
    }
    function getKinsPrice(uint256 _pid)
	external
	view
	returns (uint256 kinsPrice)
    {	
        
		PoolInfo storage pool = poolInfo[_pid];
						
		return _getKinsPrice(pool.lpToken, pool.token0, pool.token1,pool.token0_decimal, pool.token1_decimal);
    }
	
    function _getKinsPrice(address _lpToken, IERC20 _token0, IERC20 _token1,uint256 _dec0, uint256 _dec1)
	internal
	view
	returns (uint256)
    {
        uint256 kinsPrice;
		(uint256 r0, uint256 r1,) = IUniswapPair(_lpToken).getReserves();
		
		//KINS price when the KINS is token0.
		
		if(kins == address(_token0))
		{
            uint256 pow = 18;
            
		   //Check decimal difference of the tokens in pair.
            if(_dec0 > _dec1){
		        pow = 18 + _dec0 - _dec1;
		    }
		    if (_dec0 < _dec1){
		       
		       pow = 18 + _dec1 - _dec0;
		    }
		   
			kinsPrice = ( (10 ** pow) * (r1) ) / (r0); //Price of KINS in secondary token in the pair.
			
		}
		
		//KINS price when the KINS is token1.
		
		else if(kins == address(_token1))
		{
            uint256 pow = 18;
            
		   //Check decimal difference of the tokens in pair.
            if(_dec0 > _dec1){
		        pow = 18 + _dec0 - _dec1;
		    }
		    if (_dec0 < _dec1){
		       
		       pow = 18 + _dec1 - _dec0;
		    }
		   
			kinsPrice = ( (10 ** pow) * (r0) ) / (r1); //Price of KINS in secondary token in the pair.
			
		}
		
		else {
		    kinsPrice =0;
		}
						
		return kinsPrice;
    }
    function getReserves(uint256 _pid)
	external
	view
	returns (uint256 r0, uint256 r1)
    {	
        PoolInfo storage pool = poolInfo[_pid];
        
		(r0, r1,) = IUniswapPair(pool.lpToken).getReserves();
		return (r0, r1);
    }
	
}